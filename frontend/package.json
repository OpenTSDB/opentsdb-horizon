  "scripts": {
    "ng": "ng",
    "start": "ng build --watch --source-map --aot",
    "startverbose": "ng build --watch --source-map --extract-css --aot --verbose",
    "clidebug": "node --inspect ./node_modules/@angular/cli/bin/ng build --watch --source-map --extract-css --aot",
    "build": "ng build --configuration production --aot",
    "test": "ng test --watch=false --code-coverage && rm -rf ./node_modules/uws/*.node && rm -rf ./node_modules/node-sass/vendor/darwin-x64-57/binding.node && rm -rf ./node_modules/node-sass/vendor/linux-x64-57/binding.node",
    "lint": "ng lint",
    "qlint": "ng lint --quiet",
    "e2e": "ng e2e",
    "build:horizon": "ng build horizon --configuration production --output-hashing=none && node build-single.js",
    "pretty": "prettier --write \"./**/*.{js,jsx,mjs,cjs,ts,tsx,json}\"",
    "eslintfix": "eslint --fix \"./**/*.{js,jsx,mjs,cjs,ts,tsx,json}\""
  },
